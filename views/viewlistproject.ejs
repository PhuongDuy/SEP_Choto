<html class="no-js" lang="">
<!--<![endif]-->

  <%- include ./partials/header%>


<body>
    <div class="contact col-md-12">
      </div>
  <div class="container">
    <div class="row detail">
        <div class="main-breadcrumb col-sm-12">
            <ol class="breadcrumb">
              <li><a href="/">Trang chủ</a></li>
              <li><a href="/viewlistproject/<%= ty._id%>"><%= ty.TypeName%></a></li>
            </ol>
      </div>
      <% data.forEach(function(item){%>
      <div class="col-xs-6 col-md-3" >
        <div class="thumbnail" style="margin-top: 20px">
          <!-- kích thước ảnh 171*180px -->
          <a href="/viewdetail/<%= item._id%>">
            <img src="../frontend/images/<%= item.Image %>" style="width: 360px;height: 200px;">
          </a>
          <div class="caption-a">
            <h4><%=item.ProductName.substring(0,20)%></h4>
            <p><%=item.Discription.substring(0,30)%>...</p>
            <p style="color: #fb8004;font-size: 18px;display: block;font-weight: 600"><%= item.Price%>
              <sup>đ</sup></p>
            <p>
              <a href="/viewdetail/<%= item._id%>" class="btn btn-primary" role="button">Xem chi tiết</a>
          </div>
        </div>
      </div>
      <%})%>
    </div>
    <!-- pagination -->
    <nav aria-label="Page navigation" style="text-align: center">
      <ul class="pagination">
        <li>
          <a href="#" aria-label="Previous">
            <span aria-hidden="true">&laquo;</span>
          </a>
        </li>
        <li>
          <a href="#">1</a>
        </li>
        <li>
          <a href="#">2</a>
        </li>
        <li>
          <a href="#">3</a>
        </li>
        <li>
          <a href="#">4</a>
        </li>
        <li>
          <a href="#">5</a>
        </li>
        <li>
          <a href="#" aria-label="Next">
            <span aria-hidden="true">&raquo;</span>
          </a>
        </li>
      </ul>
    </nav>
  </div>

</body>


  <%- include ./partials/footer %>


</html>